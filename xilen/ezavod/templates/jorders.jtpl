            <table class="table tablex" style="width:100%; text-align:left">
					<tr>
						<th class="first_td">№</th>
						<th>Товар</th>
						<th>Кол-во</th>
						<th>Ед.</th>
						<th>Цена</th>
						<th>Сумма</th>
