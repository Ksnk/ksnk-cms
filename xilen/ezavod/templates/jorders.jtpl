{% macro searchform(data=0,pages=0) %}
##
## табличка про панель управления
##
<form name="searchform" action="" method="POST">
<table class="long tahoma tablex" style="margin:20px 0;"><tr>
<th>статус заказа</th><td><select name="status">
<option value="0">Все</option>
<option value="1">Активен</option>
<option value="2">Неактивный</option>
<option value="3">Оплачен</option>
</select></td><td></td>
<th>Способ оплаты</th><td><select name="type">
<option value="0">все</option>
<option value="1">Безналичный</option>
<option value="2">Квитанция</option>
<option value="3">Наличный</option>
</select></td><td></td>
<th>Период</th><td><select name="period">
<option value="0">все</option>
<option value="1">неделя</option>
<option value="2">месяц</option>
<option value="3">год</option>
</select></td>
<td><input type="submit" value="Искать">
</td></tr></table></form>
{% endmacro %}
#########################################################################
##
##  форма вывода списка
##
#########################################################################

{% macro orderlist(data=0,pages=0) %}
{% if pages %}<div class="pages" >{{pages}}</div> {% endif %}
<div style="width:100%;height:100%;overflow:auto;">
    {% if data|length %}
<table class="fixed long tahoma table"><col><col><col><col>
    <tr class="even align_left">## заголовок таблицы
        <th>Дата заказа</th>
        <th>Состояние </th>
        <th>Тип заказа</th>
        <th>Цена</th>
    </tr>
    {% for row in data %}
    {%  set class = loop.cycle('odd', 'even') %}
    <tr style="cursor:pointer;" class="{{class}}" onclick="$(this).next('tr').toggle();">## первая строчка заказа
        ##
        ## метка для массовых операций с заказами
        ##
        <td>{{row.date}}</td>
        <td>{{row.status}}</td>
        <td>{{row.type}}</td>
        <td>{{row.cost}}</td>
    </tr>
    <tr class="{{class}}" style='display:none;'>
        <td colspan=4 >
        {% set order =row.order %}
                {% if order.llist %}
                <table class="long"><td>
             <table  style="text-align:left">
					<tr>
						<td class="first_td">№</td>
						<td>Товар</td>
						<td>Кол-во</td>
						<td>Ед.</td>
						<td>Цена</td>
						<td>Сумма</td>
					</tr>
					{% for i in order.llist %}
					<tr>
						<td class="first_td">{{i.numb}}</td>
						<td>{{i.name}}</td>
						<td>{{i.cnumb}}</td>
						<td>шт.</td>
						<td>{{i.ccost}}</td>
						<td>{{i.cccost}}</td>
					</tr>
					{% endfor %}
					<tr>
						<td class="first_td" colspan=5 align=right>Итого:</td>
						<td align=left>{{order.summ}}</td>
					</tr>
				</table>
                    </td><td align="right">
                <table class="tahoma">
                    {% if order.cust_FIO %}<tr><td>ФИО</td><td>{{order.cust_FIO}}</td></tr>{% endif %}
                    {% if order.address %}<tr><td>Адрес</td><td>{{order.address}}</td></tr>{% endif %}
                    {% if order.cust_PHONE %}<tr><td>Телефон</td><td>{{order.cust_PHONE}}</td></tr>{% endif %}
                    {% if order.cust_EMAIL %}<tr><td>email</td><td>{{order.cust_EMAIL}}</td></tr>{% endif %}
                    {% if order.cust_ORGANISATION %}<tr><td>Организация</td><td>{{order.cust_ORGANISATION}}</td></tr>{% endif %}
                </table>
                    </td></table>
                {% endif %}
        </td>
    </tr>
    {% endfor %}
</table>
    {% else %}
    Нет сохраненных заказов
    {% endif %}
</div>
{% if pages %}<div class="pages" >{{pages}}</div> {% endif %}

</form>

        
{% endmacro %}

